<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="ContratoSuite">
    <test name="Testes de Contrato - PUFV Contrato">
        <groups>
            <run>
                <include name="contrato-empresa"/>
                <include name="contrato-ente-publico-descentralizado"/>
                <include name="contrato-ente-publico-centralizado"/>
                <include name="contrato-assessor-centralizado"/>
                <include name="contrato-assessor-descentralizado"/>
                <include name="contrato-ente-privado-descentralizado"/>
                <include name="contrato-ente-privado-centralizado"/>
            </run>
        </groups>

        <packages>
            <package name="contrato.*"/>
        </packages>
    </test>
</suite>